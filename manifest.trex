<?xml version="1.0" encoding="utf-8" ?>
<ExtensionManifest xmlns="http://tableau.com/2018/extensions/extension-manifest">

  <Name>Tableau Chat Assistant</Name>
  <Version>1.0.0</Version>
  <Author>
    <Name>Haseeb</Name>
    <Email>your@email.com</Email>
  </Author>
  <Description>Chatbot assistant using Tableau data and OpenAI</Description>

  <!-- The URL of your hosted chat.html (must be HTTPS) -->
  <WebpageUrl>https://yourdomain.com/chat.html</WebpageUrl>

  <Extension id="tableau-chat-assistant">
    <Script>chat.html</Script>
    <UseDialog>false</UseDialog>
  </Extension>

  <!-- Permissions for filter and datasource if needed -->
  <RequiredPermissions>
    <Permission>datasources</Permission>
    <Permission>filters</Permission>
  </RequiredPermissions>

</ExtensionManifest>
